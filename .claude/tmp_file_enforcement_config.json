{
  "enforcement_level": "strict",
  "enforcement_permanent": true,
  "survives_session_reset": true,
  "forbidden_patterns": [
    "['\\\"]/?tmp['\\\"]",
    "['\\\"]/?tmp/",
    "['\\\"]temp['\\\"]",
    "['\\\"]/temp['\\\"]",
    "['\\\"]tmp['\\\"]",
    "['\\\"]/tmp['\\\"]",
    "Path\\(.*['\\\"]tmp['\\\"]",
    "Path\\(.*['\\\"]temp['\\\"]",
    "open\\(['\\\"]tmp",
    "open\\(['\\\"]/tmp",
    "mkdir\\(['\\\"]tmp",
    "mkdir\\(['\\\"]/tmp",
    "chdir\\(['\\\"]tmp",
    "chdir\\(['\\\"]/tmp",
    "getcwd\\(\\).*tmp",
    "exists\\(['\\\"]tmp",
    "exists\\(['\\\"]/tmp"
  ],
  "allowed_patterns": [
    "tmp_dir = project_root / \"tmp\"",
    "project_root / \"tmp\"",
    "get_database_path\\(\"testing\"\\)",
    "project_root/tmp"
  ],
  "allowed_functions": [
    "tempfile.mkdtemp",
    "tempfile.mkstemp",
    "tempfile.NamedTemporaryFile",
    "tempfile.TemporaryDirectory",
    "tempfile.gettempdir",
    "pathlib.Path(tempfile.gettempdir())"
  ],
  "required_imports": [
    "tempfile",
    "pathlib.Path"
  ],
  "ignored_files": [
    ".venv/",
    "venv/",
    "__pycache__/",
    "htmlcov/",
    ".git/",
    "build/",
    "dist/",
    ".pytest_cache/",
    ".coverage",
    "*.pyc",
    "*_test.py",
    "test_*.py",
    "conftest.py",
    "setup.py",
    "Makefile",
    "Dockerfile",
    "docker-compose*",
    "tests/helpers/database_helper.py"
  ],
  "global_ignores": [
    ".venv",
    "venv",
    "htmlcov",
    "__pycache__",
    ".git",
    ".pytest_cache",
    "build",
    "dist",
    ".coverage",
    "*.egg-info"
  ]
}
