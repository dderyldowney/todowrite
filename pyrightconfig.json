{
    "pythonVersion": "3.12",
    "pythonPlatform": "Darwin",
    "typeCheckingMode": "standard",
    "reportMissingImports": "warning",
    "reportMissingTypeStubs": "warning",
    "include": ["lib_package/src/todowrite", "cli_package/src/todowrite_cli"],
    "exclude": [
        ".venv",
        "__pycache__",
        "build",
        "dist",
        ".git",
        "CLAUDE_SUBAGENTS_PACKAGE",
        "tests/**",
        "**/test_*.py",
        "**/_test.py",
        "**/conftest.py"
    ],
    "ignore": [],
    "useLibraryCodeForTypes": true,
    "enableTypeIgnoreComments": false,
    "stubPath": "typings",
    "executionEnvironments": [
        {
            "root": ".",
            "pythonVersion": "3.12",
            "pythonPlatform": "Darwin",
            "extraPaths": [
                "lib_package/src",
                "cli_package/src"
            ]
        }
    ]
}
